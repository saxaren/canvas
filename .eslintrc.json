{
  "env": {
    "react-native/react-native": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-native"],
  "root": true,
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
